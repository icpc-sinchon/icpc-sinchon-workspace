  "components": {
    "schemas": {
      "SemesterEntity": {
        "type": "object",
        "properties": {
          "season": {
            "type": "string",
            "enum": [
              "Spring",
              "Summer",
              "Fall",
              "Winter"
            ]
          },
          "id": {
            "type": "number"
          },
          "year": {
            "type": "number"
          }
        },
        "required": [
          "season",
          "id",
          "year"
        ]
      },
      "CreateSemesterDto": {
        "type": "object",
        "properties": {
          "season": {
            "type": "string",
            "enum": [
              "Spring",
              "Summer",
              "Fall",
              "Winter"
            ]
          },
          "year": {
            "type": "number"
          }
        },
        "required": [
          "season",
          "year"
        ]
      },
      "UpdateSemesterDto": {
        "type": "object",
        "properties": {
          "season": {
            "type": "string",
            "enum": [
              "Spring",
              "Summer",
              "Fall",
              "Winter"
            ]
          },
          "year": {
            "type": "number"
          }
        }
      },
      "LectureEntity": {
        "type": "object",
        "properties": {
          "level": {
            "type": "string",
            "enum": [
              "Novice",
              "Intermediate",
              "Advanced"
            ]
          },
          "id": {
            "type": "number"
          },
          "lectureNumber": {
            "type": "number"
          },
          "bojGroupId": {
            "type": "number"
          },
          "semesterId": {
            "type": "number"
          }
        },
        "required": [
          "level",
          "id",
          "lectureNumber",
          "bojGroupId",
          "semesterId"
        ]
      },
      "CreateLectureDto": {
        "type": "object",
        "properties": {
          "level": {
            "type": "string",
            "enum": [
              "Novice",
              "Intermediate",
              "Advanced"
            ],
            "description": "강의 난이도 (Novice, Intermediate, Advanced)"
          },
          "lectureNumber": {
            "type": "number",
            "minimum": 1,
            "description": "강의 회차수 (기본값: 10)"
          },
          "bojGroupId": {
            "type": "number",
            "description": "BOJ 그룹 ID"
          },
          "semesterId": {
            "type": "number",
            "minimum": 1,
            "description": "학기 ID (Semester의 id 값)"
          }
        },
        "required": [
          "level",
          "bojGroupId",
          "semesterId"
        ]
      },
      "UpdateLectureDto": {
        "type": "object",
        "properties": {}
      },
      "TaskEntity": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "round": {
            "type": "number"
          },
          "practiceId": {
            "type": "number"
          },
          "lectureId": {
            "type": "number"
          },
          "minSolveCount": {
            "type": "number"
          }
        },
        "required": [
          "id",
          "round",
          "practiceId",
          "lectureId",
          "minSolveCount"
        ]
      },
      "CreateTaskDto": {
        "type": "object",
        "properties": {
          "round": {
            "type": "number",
            "description": "강의 회차 (round)"
          },
          "practiceId": {
            "type": "number",
            "description": "BOJ 그룹 내 연습 ID (practiceId)"
          },
          "lectureId": {
            "type": "number",
            "description": "연결된 강의 ID (Lecture의 id 값)"
          },
          "minSolveCount": {
            "type": "number",
            "description": "출석 여부를 판단하는 최소 문제 해결 수"
          }
        },
        "required": [
          "round",
          "practiceId",
          "lectureId",
          "minSolveCount"
        ]
      },
      "CreateProblemDto": {
        "type": "object",
        "properties": {
          "bojProblemNumber": {
            "type": "number",
            "description": "BOJ 문제 번호"
          },
          "essential": {
            "type": "boolean",
            "description": "필수 문제 여부"
          },
          "taskId": {
            "type": "number",
            "minimum": 1,
            "description": "과제 ID (Task의 id 값)"
          }
        },
        "required": [
          "bojProblemNumber",
          "essential",
          "taskId"
        ]
      },
      "UpdateTaskDto": {
        "type": "object",
        "properties": {
          "round": {
            "type": "number",
            "description": "강의 회차 (round)"
          },
          "practiceId": {
            "type": "number",
            "description": "BOJ 그룹 내 연습 ID (practiceId)"
          },
          "lectureId": {
            "type": "number",
            "description": "연결된 강의 ID (Lecture의 id 값)"
          },
          "minSolveCount": {
            "type": "number",
            "description": "출석 여부를 판단하는 최소 문제 해결 수"
          },
          "problems": {
            "description": "문제 배열 (BOJ 문제 정보)",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CreateProblemDto"
            }
          }
        }
      },
      "ProblemEntity": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "bojProblemNumber": {
            "type": "number"
          },
          "essential": {
            "type": "boolean"
          },
          "taskId": {
            "type": "number"
          }
        },
        "required": [
          "id",
          "bojProblemNumber",
          "essential",
          "taskId"
        ]
      },
      "UpdateProblemDto": {
        "type": "object",
        "properties": {
          "bojProblemNumber": {
            "type": "number",
            "description": "BOJ 문제 번호"
          },
          "essential": {
            "type": "boolean",
            "description": "필수 문제 여부"
          },
          "taskId": {
            "type": "number",
            "minimum": 1,
            "description": "과제 ID (Task의 id 값)"
          }
        }
      }
    }
  }
